{
  "compilerOptions": {
    "outDir": "dist",
    "noErrorTruncation": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "sourceMap": true,
    "newLine": "LF",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "downlevelIteration": true,
    "declaration": true,
    "stripInternal": true,
    "target": "es6",
    "lib": [
      "es5",
      "es2015.collection",
      "es2015.iterable"
    ]
  },
  "exclude": [
    "dist",
    "node_modules",
    "generated/**",
    "test/**"
  ]
}
